<div class="container">
  <div class="row">
    <% @project.videos.each do |video| %>
      <div class="col-md-4 col-sm-6">
        <%= video_tag video, controls: false, autoplay: true, loop: true, muted: true, class: "#{@project.category} img-fluid mt-3 mb-5" %>
      </div>
    <% end %>
  </div>
</div>
