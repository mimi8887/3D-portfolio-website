  <div class="title">
    <h4><%= @project.name %></h4>
  </div>
  <div class="description mt-3">
    <p><%= @project.description %></p>

      <% @project.videos.each do |video| %>
        <%= video_tag video, controls: false, autoplay: true, loop: true, class: "#{@project.category} img-fluid mt-3" %>
      <% end %>
</div>
